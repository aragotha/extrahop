    "Trigger": [
        {
            "apply_all": false, 
            "author": "Archana", 
            "comment": "", 
            "debug": false, 
            "disabled": false, 
            "events": "[\"SSL_CLOSE\", \"SSL_OPEN\", \"SSL_RECORD\"]", 
            "hints": "{}", 
            "name": "SaaS applications", 
            "script": "var appName = 'SaaS'; \n\n\nvar sslCheck = [\n\t\n\t{ cert: 'webex.com', name: ': webex'},\n    { cert: 'sharepoint.com', name: ': sharepoint'},\n\t{ cert: 'webexconnect.com', name: ': webex'},\n\t{ cert: 'outlook.com', name: ': outlook'},\n\t{ cert: 'outlook.office.com', name: ': outlook'},\n\t{ cert: 'office365.com', name: ': office365'},\n\t{ cert: 'azure.com', name: ': azure'},\n\t{ cert: 'amazonaws.com', name: ': amazonaws'},\n\t{ cert: 'officeapps.live.com', name: ': officeapps.live'},\n\t{ cert: 'microsoftonline.com', name: ': microsoftonline'},\n\t{ cert: 'office.com', name: ': office.com'},\n\t{ cert: 'office.net', name: ': office.net'},\n\t{ cert: 'microsoft.com', name: ': microsoft.com'},\n\t{ cert: 'yammer.com', name: ': yammer'},\n    { cert: 'service-now', name: ': service-now'}\n\n];\n\nswitch (event)\n{\n    case 'SSL_OPEN':\n        if (SSL.certificate && SSL.certificate.subject) {\n            var cert = SSL.certificate.subject;\n        //  debug('ssl heartbeat: ' + SSL.certificate.subject);\n            for (var i = 0; i < sslCheck.length; i++) {    \n\t\t\t    if (cert.indexOf(sslCheck[i].cert) != -1) {                     \n                    appName += sslCheck[i].name;\n                    //debug(\"SSL_FLOW_APPNAME\" + \": \" + appName);\n                    Flow.addApplication(appName, true);\n                }\n            }\n\n        }\n        break;\n    case 'SSL_RECORD':\n    case 'SSL_CLOSE':\n        if (SSL.certificate && SSL.certificate.subject) {\n            var cert = SSL.certificate.subject;\n            for (var i = 0; i < sslCheck.length; i++) {    \n\t\t\t    if (cert.indexOf(sslCheck[i].cert) != -1) {                     \n                    appName += sslCheck[i].name;\n                    //debug(\"SSL_APPNAME\" + \": \" + appName);\n                    Application(appName).commit()\n                }\n            }   \n            \n        }\n        break;\n    \n    \n\n}"
        }
    ], 
